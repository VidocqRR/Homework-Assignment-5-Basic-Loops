for num in range(1,100):
    if num % 3 == 0 and num % 5 == 0:

        print('FizzBuzz');
    elif num % 3 == 0:
        print('Fizz');
    elif num  % 5 == 0:
        print('Buzz');
    elif num % num == 0:
        print('Prime')
    else:
        print(num)
